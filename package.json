{
  "name": "stackit-sdk-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --ignore-path .prettierignore --write \"**/*.{js,jsx,ts,tsx,json,md,css,scss}\"",
    "format:check": "prettier . --ignore-path .gitignore --check",
    "lint:fix": "yarn format & next lint --dir src",
    "test": "jest --passWithNoTests",
    "test:staged": "jest --bail --findRelatedTests --watchAll=false --passWithNoTests",
    "terraform:apply": "terraform apply -auto-approve -var-file='.env.tfvars'",
    "prepare": "husky install"
  },
  "dependencies": {
    "@hookform/error-message": "^2.0.1",
    "@hookform/resolvers": "^2.9.11",
    "@next/font": "13.1.6",
    "@react-oauth/google": "^0.6.1",
    "@reduxjs/toolkit": "1.9.2",
    "@types/jest": "29.4.0",
    "@types/node": "18.11.18",
    "@types/react": "18.0.27",
    "@types/react-dom": "18.0.10",
    "axios": "1.2.6",
    "clsx": "^1.2.1",
    "daisyui": "2.49.0",
    "dayjs": "1.11.7",
    "eslint": "8.33.0",
    "eslint-config-next": "13.1.6",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-jest": "^27.2.1",
    "next": "13.1.6",
    "next-themes": "0.2.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "7.43.0",
    "react-icons": "^4.7.1",
    "react-infinite-scroll-component": "^6.1.0",
    "react-modern-drawer": "1.2.0",
    "react-redux": "8.0.5",
    "react-spinners": "^0.13.8",
    "react-toastify": "^9.1.1",
    "stackit-sdk-spinner-container": "^1.0.0",
    "swr": "^2.0.3",
    "ts-jest": "29.0.5",
    "typescript": "4.9.5"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "^17.4.4",
    "@swc/core": "1.3.31",
    "@swc/jest": "0.2.24",
    "@typescript-eslint/eslint-plugin": "5.49.0",
    "autoprefixer": "10.4.13",
    "eslint-config-standard-with-typescript": "33.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-react": "7.32.2",
    "husky": "8.0.3",
    "jest": "29.4.1",
    "lint-staged": "13.1.0",
    "postcss": "8.4.21",
    "prettier": "2.8.3",
    "standard": "17.0.0",
    "tailwindcss": "3.2.4"
  }
}
